package arrays;

public class GeneratingSubArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = {1,6,4,8,3,1,9};
		
		for(int i=0;i<arr.length;i++) {
			for(int j=i;j<arr.length;j++) {
				System.out.print(arr[j]+" ");
				
			}
			System.out.println();
		}

	}

}
