package corejava;

class A{
	public void add(int a,int b) {
		System.out.println("In method 1");
	}
	public void add(double a,int b) {
		System.out.println("In method 1");
	}
}
public class MethodOverLoading {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new A().add(1.0, 1);
	}

}
